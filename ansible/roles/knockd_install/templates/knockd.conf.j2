[options]
     UseSyslog
     Interface = {{ nic_id }}

[SSH]
     sequence = 7000,8000,9000
     seq_timeout = 5
     tcpflags = syn
     start_command = /sbin/iptables -I INPUT -s {{ remote_ip }} -p tcp --dport 22 -j ACCEPT
     stop_command = /sbin/iptables -D INPUT -s {{ remote_ip }} -p tcp --dport 22 -j ACCEPT
     cmd_timeout = 60
